{
  "name": "cause-cli",
  "description": "Prototype compiler and runner for the Cause language",
  "version": "0.0.0",
  "author": "Dallon Feldner @dallonf",
  "bin": {
    "cause": "./bin/run"
  },
  "bugs": "https://github.com/dallonf/causelang/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@tsconfig/node12": "^1.0.7",
    "causelang-proto-compiler": "^1.0.0",
    "cli-ux": "^5.5.1",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "^14.14.41",
    "eslint": "^7.25.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dallonf/causelang",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cause",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "dallonf/causelang",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
